{
    "family": "react-app-ecs",
    "containerDefinitions": [
        {
            "name": "react-app",
            "image": "841738364964.dkr.ecr.us-east-1.amazonaws.com/react-app:aws-reactjs_demo_image",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "container-port",
                    "containerPort": 3001,
                    "hostPort": 3001,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/react-app-ecs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::841738364964:role/AWS-ECS-TaskExecution-Role",
    "executionRoleArn": "arn:aws:iam::841738364964:role/AWS-ECS-TaskExecution-Role",
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "500",
    "memory": "450",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}